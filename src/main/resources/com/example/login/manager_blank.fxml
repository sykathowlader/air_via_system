            <Button layoutX="2.0" layoutY="241.0" mnemonicParsing="false" onAction="#goToManagerCommission" prefHeight="48.0" prefWidth="115.0" style="-fx-background-color: transparent; -fx-border-color: #4350b5; -fx-border-width: 0px 0px 2px 0px;" text="Commission" />
            <Button layoutY="289.0" mnemonicParsing="false" onAction="#goToManagerAlert" prefHeight="48.0" prefWidth="115.0" style="-fx-background-color: transparent; -fx-border-color: #4350b5; -fx-border-width: 0px 0px 2px 0px;" text="Alert" />
         </children>
      </AnchorPane>
      <Button layoutX="400.0" layoutY="296.0" mnemonicParsing="false" onAction="#removeBlank" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #18b0b3;" text="Remove" />
      <Button layoutX="300.0" layoutY="296.0" mnemonicParsing="false" onAction="#assignBlankToAdvisor" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #18b0b3;" text="Assign" />
      <Label layoutX="205.0" layoutY="161.0" text="From Blank" />
