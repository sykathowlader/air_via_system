            <Button layoutY="97.0" mnemonicParsing="false" onAction="#goToManagerReport" prefHeight="48.0" prefWidth="115.0" style="-fx-background-color: transparent; -fx-border-color: #4350b5; -fx-border-width: 0px 0px 2px 0px;" text="Report" />
            <Button layoutY="145.0" mnemonicParsing="false" onAction="#goToManagerDiscount" prefHeight="48.0" prefWidth="115.0" style="-fx-background-color: transparent; -fx-border-color: #4350b5; -fx-border-width: 0px 0px 2px 0px;" text="Manage Discount" />
